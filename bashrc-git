alias ga='git add '
alias gb='git branch '
alias gc='git commit '
alias gcam='git commit --amend '
alias gcamne='git commit --amend --no-edit '
alias gce='git config user.email '
alias gcl='git clone '
alias gcn='git config user.name '
alias gche='git config credential.helper '"'"'cache --timeout 30000'"'"''
alias gcst='git config credential.helper store '
alias gcp='git cherry-pick '
alias gcpok='git cherry-pick --continue '
alias gcpno='git cherry-pick --abort '
alias gd='git diff '
alias gds='git diff --staged '
alias ghrd='git reset --hard '
alias gl='git log '
alias glflw='git log --follow '
alias glp='git log -p '
alias glpo='git log --pretty=oneline '
alias gm='git merge '
alias go='git checkout '
alias gpl='git pull '
alias gpal='git push --all '
alias gpfal='git push --force --all '
alias gphd='git push host --delete '
alias gphh='git push host HEAD '
alias gpfhh='git push --force host HEAD '
alias gpod='git push origin --delete '
alias gplh='git push local HEAD '
alias gpflh='git push --force local HEAD '
alias gpld='git push local --delete '
alias gpoh='git push origin HEAD '
alias gpfoh='git push --force origin HEAD '
alias gppd='git push public --delete '
alias gpph='git push public HEAD '
alias gpfph='git push --force public HEAD '
alias grh='git reset HEAD '
alias greb='git rebase '
